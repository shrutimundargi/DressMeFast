<BorderPane prefHeight="750.0" prefWidth="1200.0" styleClass="pnlHome" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <left>
      <BorderPane prefHeight="200.0" prefWidth="200.0" styleClass="leftMenu" BorderPane.alignment="CENTER">
         <bottom>
            <AnchorPane>
               <children>
                  <Button fx:id="btnUser" mnemonicParsing="false" onAction="#goStatistics" prefHeight="50.0" prefWidth="193.0" styleClass="btnUser" text="Button" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
               </children>
            </AnchorPane>
         </bottom>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" styleClass="v-box-menu" BorderPane.alignment="CENTER">
               <children>
                  <ButtonBar styleClass="btn-menu">
                     <buttons>
                        <HBox>
                           <children>
                              <ImageView fx:id="logoHomePage" fitHeight="27.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" styleClass="img-selection-menu">
                                 <HBox.margin>
                                    <Insets bottom="5.0" top="5.0" />
                                 </HBox.margin>
                              </ImageView>
                              <Label fx:id="btnMenuHome" prefHeight="36.0" prefWidth="159.0" styleClass="text-menu" text="Home">
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </Label>
                           </children>
                        </HBox>
                     </buttons>
                  </ButtonBar>
                  <ButtonBar fx:id="btnMenuBrand" styleClass="btn-menu">
                     <buttons>
                        <HBox>
                           <children>
                              <ImageView fx:id="logoBrandPage" fitHeight="27.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" styleClass="img-selection-menu">
                                 <HBox.margin>
                                    <Insets bottom="5.0" top="5.0" />
                                 </HBox.margin>
                              </ImageView>
                              <Label prefHeight="36.0" prefWidth="157.0" styleClass="text-menu" text="Brand" />
                           </children>
                        </HBox>
                     </buttons>
                  </ButtonBar>
                  <ButtonBar fx:id="btnMenuAdd" styleClass="btn-menu">
                     <buttons>
                        <HBox>
                           <children>
                              <ImageView fx:id="logoAddPage" fitHeight="27.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" styleClass="img-selection-menu">
                                 <HBox.margin>
                                    <Insets bottom="5.0" top="5.0" />
                                 </HBox.margin>
                              </ImageView>
                              <Label prefHeight="36.0" prefWidth="156.0" styleClass="text-menu" text="Add +" />
                           </children>
                        </HBox>
                     </buttons>
                  </ButtonBar>
                  <ButtonBar fx:id="btnMenuFavorite" styleClass="btn-menu">
                     <buttons>
                        <HBox>
                           <children>
                              <ImageView fx:id="logoFavoritePage" fitHeight="27.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" styleClass="img-selection-menu">
                                 <HBox.margin>
                                    <Insets bottom="5.0" top="5.0" />
                                 </HBox.margin>
                              </ImageView>
                              <Label prefHeight="36.0" prefWidth="157.0" styleClass="text-menu" text="Favorite" />
                           </children>
                        </HBox>
                     </buttons>
                  </ButtonBar>
                  <ButtonBar fx:id="btnMenuOutfits" styleClass="btn-menu">
                     <buttons>
                        <HBox>
                           <children>
                              <ImageView fx:id="logoOutfitsPage" fitHeight="27.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true" styleClass="img-selection-menu">
                                 <HBox.margin>
                                    <Insets bottom="5.0" top="5.0" />
