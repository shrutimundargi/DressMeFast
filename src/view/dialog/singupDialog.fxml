         <children>
            <StackPane GridPane.rowIndex="1">
               <children>
                  <Text fx:id="txtPopupName" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-popup" text="Text" />
               </children>
            </StackPane>
         </children>
      </GridPane>
   </center>
   <bottom>
      <BorderPane>
         <center>
            <Button mnemonicParsing="false" onAction="#goToHome" styleClass="btn-normal" text="Start">
               <BorderPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </BorderPane.margin>
            </Button>
         </center>
      </BorderPane>
   </bottom>
